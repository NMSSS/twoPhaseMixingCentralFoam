FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type sets;
libs (sampling);//("libsampling.so");
writeControl	runTime;
writeInterval	1e-3;

interpolationScheme cell;
setFormat       csv;

sets
(
    line
    {
        type    uniform;
        axis    x;
        start   ( 0.000625 0 0 );
        end     ( 9.999375 0 0 );
        nPoints 8000;
    }
);

fields          ( Ybar.Liq rho p U T);

